{% extends "template.twig" %}

{% block content %}
    <div class="container">
        <table>
            <caption>ConnectionsCounter</caption>
            <tr>
                <th>method</th>
                <th>amount</th>
            </tr>
            {% for key, value in data %}
                <tr>
                    <td>{{ key }}</td>
                    <td>{{ value }}</td>
                </tr>
            {% endfor %}
        </table>
        <br>
        <button onclick="window.open('/root', '_parent')">ROOT</button>
    </div>
{% endblock %}